{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.1" }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/" },
        { "src": "/sw.js", "dest": "/public/sw.js" },
        { "src": "/manifest.json", "dest": "/public/manifest.json" },
        { "src": "/img/(.*)", "dest": "/public/img/$1" },
        { "src": "/favicon.ico", "dest": "/favicon.ico" },
        { "src": "/offline.html", "dest": "/public/offline.html" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "public": true,
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://digital-time-capsule-pied.vercel.app",
        "ASSET_URL": "https://digital-time-capsule-pied.vercel.app",
        "FORCE_HTTPS": "true",
        "APP_KEY": "base64:UKK0eEd2/Gicg+X9nMdgKvNiP4fnGt3XvOysnWpFTF0=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "bke5ab.h.filess.io",
        "DB_PORT": "3306",
        "DB_DATABASE": "time_capsules_clawssmall",
        "DB_USERNAME": "time_capsules_clawssmall"
    }
}
